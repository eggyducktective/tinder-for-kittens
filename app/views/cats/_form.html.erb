<%= form_for @cat do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :image %>
<%= f.text_field :image %>
<br>
<%= f.label :bio %>
<%= f.text_field :bio %>
<br>
<% Hobby.all.each do |h| %>
  <%= check_box_tag('hobbies[]', h.id, multiple: true) %>
  <%= label_tag h.name %><br>
<% end %>
<%= f.submit %>
<% end  %>
