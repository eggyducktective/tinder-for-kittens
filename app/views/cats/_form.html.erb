
<%= form_for(@cat, :html => {:multipart => true}) do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>
<br>

<%= f.label :bio %>
<%= f.text_field :bio %>
<br>

<%= f.label "Cat Image:" %>
<%= f.cl_image_upload :image %>
<br>

<% Hobby.all.each do |h| %>

  <%= check_box_tag('hobbies[]', h.id, multiple: true) %>
  <%= label_tag h.name %><br>
<% end %>


<%= f.submit "Create Animal" %>
<% end  %>
